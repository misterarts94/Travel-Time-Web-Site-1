@model VehicleType

<!DOCTYPE html>
<html>
<head>
    <style>
        body {
            background-image: url("/images/bgImage/bgi (6).jpg");
            background-size: cover;

        }
    </style>
</head>
<body>
    <div class="container">
        <div class="card">
            <div class="card-header text-center text-uppercase">
                Vehicle Type Details
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="row">
                            <div class="col-md-8">
                                <p class="card-text text-uppercase"><strong>Id</strong></p>
                                <p class="card-text">@Model.Id</p>
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-md-8">
                                <p class="card-text text-uppercase"><strong>Vehicle Type</strong></p>
                                <p class="card-text">@Model.Name</p>
                            </div>
                        </div>
                        <br>

                    </div>

                </div>
                <div class="row">
                    <div class="col-md-12 text-center">
                        <a asp-area="Admin" asp-controller="VehicleType" asp-action="Index" class="btn btn-danger">Back</a>
                    </div>
                </div>
            </div>
        </div>


    </div>
    @section Scripts {
        <partial name="_ValidationScriptsPartial" />
    }

    
</body>
</html>
